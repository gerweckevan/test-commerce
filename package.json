{
  "name": "ecommerce-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production && npm run start:prod",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:prod": "node index.js",
    "heroku-postbuild": "cd client && npm run build",
    "client": "cd client && npm run start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "chart.js": "^2.9.4",
    "chokidar": "^3.5.1",
    "compression": "1.7.3",
    "dotenv": "^6.1.0",
    "express": "^4.17.1",
    "jwt-simple": "0.5.5",
    "mongoose": "^5.12.0",
    "morgan": "1.9.1",
    "node": "^14.15.0",
    "nodemailer": "4.6.8",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.1",
    "react-google-login": "^5.2.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-loadable": "^5.5.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-star-ratings": "^2.3.0",
    "reactstrap": "^8.9.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@types/dotenv": "^8.2.0",
    "concurrently": "^6.0.0",
    "nodemon": "1.18.4"
  }
}
